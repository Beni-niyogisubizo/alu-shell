#!/usr/bin/env bash
# This script stops the 4-to_infinity_and_beyond process.

# Find the PID of the 4-to_infinity_and_beyond process using pgrep
pid=$(pgrep -f "4-to_infinity_and_beyond")

# If the process is found, kill it
if [ -n "$pid" ]; then
    kill "$pid"
    echo "Process 4-to_infinity_and_beyond (PID: $pid) has been terminated."
else
    echo "Process 4-to_infinity_and_beyond not found."
fi

